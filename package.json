{
  "name": "SwiftPackageIndex",
  "repository": "git@github.com:SwiftPackageIndex/SwiftPackageIndex-Server.git",
  "js_target": "./Public/main.js",
  "css_target": "./Public/main.css",
  "targets": {
    "js_target": {
      "source": "./Resources/Scripts/main.js"
    },
    "css_target": {
      "source": "./Resources/Styles/main.scss"
    }
  },
  "scripts": {
    "build": "rollup -c Resources/rollup-prod.js",
    "serve": "rollup -c Resources/rollup-dev.js --watch"
  },
  "dependencies": {
    "@hotwired/turbo": "^7.0.0-beta.4",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "rollup": "^2.47.0",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.12"
  }
}
